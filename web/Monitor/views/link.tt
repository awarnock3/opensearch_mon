                        <td style="word-break: break-all;"> 
                            <% links.$entry.granule %>
                        </td>
                        <% ELSE %>
                        <td>
                            <span class="ui-icon ui-icon-alert"></span>
                            <span>No link</span>
                        </td>
                        <% END %>
                    </tr>
                    <% END %>
                </tbody>
