<script type="text/javascript">
 "use strict;"
 $(document).ready(function() {
     $( "#table-links" ).DataTable({
         scrollY:        '75vh',
         scrollCollapse: true,
         paging:         false,
         order: [[ 0, "asc" ]],
         columnDefs: [
             { targets: 0, width: "20%", },
